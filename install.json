{
  "cmd": { "win32": "start_windows.bat", "linux": "bash start_linux.sh", "darwin": "bash start_macos.sh" },
  "gpu": { "nvidia": ["A", "N"], "amd": "B", "apple": "C", "ipex": "D", "none": "N" },
  "clone": "git clone --depth 1 --branch v1.6 https://github.com/oobabooga/text-generation-webui.git",
  "run": [{
    "method": "shell.run",
    "params": { "message": "{{self.clone}}" }
  }, {
    "method": "shell.start",
    "params": { "path": "text-generation-webui" }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "{{self.cmd[platform]}}",
      "on": [{ "event": "/what is your gpu/gi", "return": true }]
    }
  }, {
    "method": "shell.enter",
    "params": {
      "message": "{{self.gpu[gpu]}}",
      "on": [{ "event": "/running on.*http.+/gi", "return": true }]
    }
  }, {
    "method": "input",
    "params": { "title": "Install Success", "description": "Go back to the dashboard and launch the app!" }
  }, {
    "method": "browser.open",
    "params": { "uri": "/?selected=text-generation-webui" }
  }]
}
